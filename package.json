{
    "name": "node-csv-streamer",
    "version": "0.1.0",
    "description": "Stream large datasets and export to CSV - streaming, memory-efficient exporter",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "test": "jest --runInBand",
        "lint": "eslint . --ext .ts",
        "prepare": "npm run build",
        "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
        "prepublishOnly": "npm run build && npm test"
    },
    "dependencies": {
        "fast-csv": "^4.3.6"
    },
    "keywords": [
        "stream",
        "csv",
        "streaming",
        "export",
        "data",
        "fast-csv"
    ],
    "author": "Adeleke Bright <adenababanla@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.19.23",
        "jest": "^29.7.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/adeisbright/node-csv-streamer.git"
    }
}
